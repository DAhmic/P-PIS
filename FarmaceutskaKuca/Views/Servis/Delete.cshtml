@model FarmaceutskaKuca.Models.servis

@{
    ViewBag.Title = "Delete";
}

<h2>Izbriši</h2>

<h3>Da li ste sigurni da želite izbrisati ovaj unos?</h3>
<div>
    <h4>Servis</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Naziv: ")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.naziv)
        </dd>

        <dt>
            @Html.DisplayName("RTO/RPO: ")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.RTO_RPO)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Izbriši" class="btn btn-default" /> |
            @Html.ActionLink("Nazad", "Index")
        </div>
    }
</div>
