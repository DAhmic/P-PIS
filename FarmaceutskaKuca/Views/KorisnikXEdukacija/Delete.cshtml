@model FarmaceutskaKuca.Models.korisnikxedukacija

@{
    ViewBag.Title = "Delete";
}

<h2>Izbriši</h2>

<h3>Da li ste sigurni da želite izbrisati ovaj unos?</h3>
<div>
    <h4>korisnikxedukacija</h4>
    <hr />
    <dl class="dl-horizontal">
        
        <dt>
            @Html.DisplayName("Korisnik")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.korisnik.username)
        </dd>

        <dt>
            @Html.DisplayName("Naziv edukacije")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.edukacija.naziv)
        </dd>

        <dt>
            @Html.DisplayName("Odobreno")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.odobreno)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" /> |
            @Html.ActionLink("Nazad", "Index")
        </div>
    }
</div>
